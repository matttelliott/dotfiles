#!/usr/bin/env zsh
alias g="git status"
alias gs="git status"
alias gst="git status"
alias ga="git add"
alias gc="git commit"
alias gco="git checkout"
alias gd="git diff"
alias gdiff="git diff"
alias gac="git add . && git commit"
alias gacm="git add . && git commit -m"
alias gpush="git push"
alias gpull="git pull"
alias gfetch="git fetch"
alias gf="git fetch"
